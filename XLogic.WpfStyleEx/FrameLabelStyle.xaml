<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:xc="clr-namespace:XLogic.WpfControl;assembly=XLogic.WpfControl">

    <Style TargetType="xc:FrameLabel">
        <Setter Property="TextColor" Value="#8C8C8C"/>
        <Setter Property="FrameLineColor" Value="#323232"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Direction" Value="UpToDown"/>
    </Style>

</ResourceDictionary>